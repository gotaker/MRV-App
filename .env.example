# Copy this file to .env and adjust values as needed.
REGISTRY=ghcr.io
IMAGE_NAMESPACE=your-org-or-user
APP_NAME=mrv

# Will be kept in sync by `npm version` -> scripts/update-env.mjs
APP_VERSION=0.1.0

# Populated by update-env.mjs (fallbacks if not in a git repo)
GIT_COMMIT=dev
BUILD_DATE=1970-01-01T00:00:00Z

# Optional: repo URL for OCI label
VCS_URL=https://github.com/${IMAGE_NAMESPACE}/MRV-App
