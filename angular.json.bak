{
  "$schema": "https://json.schemastore.org/angular-cli",
  "version": 1,
  "projects": {
    "mrv-angular20": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/mrv-angular20",
            "index": "src/index.html",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "inlineStyleLanguage": "scss",
            "browser": "src/main.ts",
            "tsConfig": "tsconfig.app.json"
          },
          "configurations": {
            "development": {
              "optimization": false,
              "sourceMap": true
            },
            "production": {
              "optimization": true,
              "sourceMap": false
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "mrv-angular20:build:development",
            "host": "0.0.0.0",
            "port": 4200,
            "proxyConfig": "proxy.conf.json",
            "browserTarget": "mrv-angular20:build:development"
          },
          "configurations": {
            "development": {
              "browserTarget": "mrv-angular20:build:development"
            },
            "production": {
              "browserTarget": "mrv-angular20:build:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "defaultConfiguration": "development"
      }
    }
  },
  "cli": {
    "analytics": false
  }
}